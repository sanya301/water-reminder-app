//manifest for extension
{
  "name": "Hydrate",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "My first Chrome extension.",
  "permissions": [
          "storage",
          "alarms",
          "notifications",
          "background"
        ],

  "background": {
      "scripts": ["background.js"]
    },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}